{
    "env": {
        "browser": true,
        "es2021": true
    },
        "extends": "eslint:recommended",
        "parser": "vue-eslint-parser",
        "parserOptions": {
            "sourceType": "module",
            "ecmaVersion": 2018,
            "ecmaFeatures": {
                "globalReturn": false,
                "impliedStrict": false,
                "jsx": false
            }
        },
    "plugins": [
        "vue"
    ],
    "rules": {
        "data-bindings":1
    }
}
